modules = ["nodejs-20", "web", "postgresql-16"]
run = "npm run dev"

[nix]
channel = "stable-24_05"

[deployment]
run = ["sh", "-c", "NODE_ENV=production PORT=8080 npx tsx src/server/index.ts"]
build = ["sh", "-c", "npm run build && npx prisma generate && npx prisma db push"]
deploymentTarget = "cloudrun"

